# _defaults:
#   broker_instance: /opt/artemis-2.18.0-i0/

bootstrap_xml:
  jaas_security:
    - domain: activemq
  server:
    configuration: 'file:{% if ":" in broker_instance %}/{% endif %}{{ broker_instance }}/etc/broker.xml'
  web:
    bind:
      protocol: http
      address: localhost
      port: 8161
    path: web
    apps:
      {{ bootstrap_apps }}